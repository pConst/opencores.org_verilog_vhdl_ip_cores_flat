Import('env')

e = env.Clone()
e.Append(CPPPATH = ['include'])
e.Append(CCFLAGS = '-nostdinc')

objects = e.StaticObject(Glob('src/*.c'))
library = e.StaticLibrary('elf', objects)

Return('library')
