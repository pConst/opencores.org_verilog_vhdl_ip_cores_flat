bochs486:
	g++ -g -o bochs486 -I. -Iinstrument/stubs -Icpu main.cpp cpu/cpu.cc cpu/init.cc cpu/fetchdecode.cc cpu/resolver.cc cpu/logical8.cc cpu/logical16.cc cpu/logical32.cc cpu/arith8.cc cpu/arith16.cc cpu/arith32.cc cpu/access.cc cpu/access32.cc cpu/paging.cc cpu/load.cc cpu/flag_ctrl_pro.cc cpu/proc_ctrl.cc cpu/segment_ctrl_pro.cc cpu/crregs.cc cpu/generic_cpuid.cc cpu/mult8.cc cpu/mult16.cc cpu/mult32.cc cpu/bcd.cc cpu/flag_ctrl.cc cpu/data_xfer8.cc cpu/data_xfer16.cc cpu/data_xfer32.cc cpu/bit.cc cpu/bit16.cc cpu/bit32.cc cpu/shift8.cc cpu/shift16.cc cpu/shift32.cc cpu/stack16.cc cpu/stack32.cc cpu/soft_int.cc cpu/string.cc cpu/io.cc cpu/segment_ctrl.cc cpu/protect_ctrl.cc cpu/ctrl_xfer16.cc cpu/ctrl_xfer32.cc cpu/ctrl_xfer_pro.cc cpu/call_far.cc cpu/jmp_far.cc cpu/iret.cc cpu/ret_far.cc cpu/vm8086.cc cpu/tasking.cc cpu/icache.cc cpu/stack.cc cpu/event.cc cpu/fpu_emu.cc gui/paramtree.cc cpu/exception.cc

